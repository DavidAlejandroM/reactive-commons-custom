  "module": "dist/reactive-commons-custom.esm.js",
  "size-limit": [
    {
      "path": "dist/reactive-commons-custom.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/reactive-commons-custom.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "husky": "^4.2.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  }
}